#include<iostream>
#include<bits/stdc++.h>
using namespace std;

void swap(int*a,int*b){
    int temp=*a;
    *a=*b;
    *b=temp;
    return;
}
void bubblesort(int *arr,int n){jey
    int count=0;
while(count<n){
    int i=0,j=1;
    while(i<n-count){
        if(arr[i]>arr[j]){
            swap(arr[i],arr[j]);
        }
        i++;
        j++;
    }
    count++;
}
    return;
}
int main(){
    int arr[]={10,2,18,4,5,17,2,22,3,8};
    bubblesort(arr,9);
    for(int i=0;i<9;i++)
    cout<<arr[i]<<" ";
    return 0;
}
